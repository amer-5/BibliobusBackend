{
  "name": "bibliobusbackend",
  "version": "0.0.1",
  "description": "Backend za Bibliobus, digitalnu biblioteku Travnika. Omogućava: Digitalne članske karte (QR kod), Rezervacije i posudbe knjiga, Praćenje bibliobusa i rasporeda, Administraciju kataloga knjiga i korisnika",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec \"node --loader ts-node/esm src/index.ts\"",
    "start": "node dist/index.js",
    "build": "tsc",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amer-5/BibliobusBackend.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "vercel": "^48.1.6",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "@vercel/node": "^5.3.24",
    "nodemon": "^3.1.10",
    "prisma": "^6.16.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
